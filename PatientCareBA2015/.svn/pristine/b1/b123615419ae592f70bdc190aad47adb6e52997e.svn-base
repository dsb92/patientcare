package acdmihaba.personaleapp.personaleapplication.model;


/**
 * Created by Camilla Marsi on 08-09-2015.
 */
public class Item {
    private String name;
    private String type;
    private String intentAction;
    private int resultCode;

    public Item(String demoName, String type){

        this(demoName, type, null, 0);
    }

    public Item(String itemName, String itemType, String itemAction, int itemResultCode){

        this.name = itemName;
        this.type = itemType;
        this.intentAction = itemAction;
        this.resultCode = itemResultCode;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getType() {
        return type;
    }

    public void setType(String description) {
        this.type = description;
    }

    public String getIntentAction() {
        return intentAction;
    }

    public void setIntentAction(String intentAction) {
        this.intentAction = intentAction;
    }

    public int getResultCode() {
        return resultCode;
    }

    public void setResultCode(int resultCode) {
        this.resultCode = resultCode;
    }







}
